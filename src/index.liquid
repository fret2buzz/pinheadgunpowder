<section id="band" class="scroll-mt-25">
    <h1 class="sr-only">Band</h1>
    <div class="grid grid-cols-12 gap-4">
        {%- assign bandData = band | reverse -%}
        {%- for item in bandData -%}
            {%- include 'bandItem' -%}
        {%- endfor -%}
    </div>
</section>
